{
  "include": ["src"],

  "exclude": ["**/node_modules", "**/__pycache__"],

  "ignore": [],

  "stubPath": "src/typestubs",
  "venvPath": ".venv",

  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "disableOranizeImports": true,

  "pythonVersion": "3.9",
  "typeCheckingMode": "basic",
  "executionEnvironments": [
    {
      "root": "./",
      "pythonVersion": "3.9",
      "extraPaths": ["src"]
    }
  ]
}
